{
  "name": "@byc/electron-auto-restart",
  "type": "module",
  "version": "0.0.5",
  "private": false,
  "packageManager": "pnpm@9.15.9",
  "description": "",
  "author": "byc",
  "license": "ISC",
  "exports": {
    ".": {
      "dev": "./core/index.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./out/main/index.cjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --fix",
    "dev": "run-p dev:play-main dev:play-preload",
    "dev:play-main": "pnpm --filter playground-main run dev",
    "dev:play-preload": "pnpm --filter playground-preload run dev",
    "pkg:publish": "pnpm build & pnpm publish"
  },
  "dependencies": {
    "@types/proper-lockfile": "^4.1.4",
    "proper-lockfile": "^4.1.2",
    "read-pkg": "^9.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.2",
    "@rsbuild/core": "^1.2.19",
    "@types/node": "^22.13.10",
    "electron": "^35.0.3",
    "eslint": "^9.22.0",
    "npm-run-all2": "^7.0.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
